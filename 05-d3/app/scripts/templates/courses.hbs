<h2>Courses</h2>
<p>
  List of courses.
</p>
<div class="row">
  <div class="col-sm-12">
    <svg id="chart" viewBox="{{viewBox}}">
      <g transform="{{transformG}}">
        <g class="x axis" transform="{{transformX}}"></g>
        <g class="rects"></g>
      </g>
    </svg>
  </div>
</div>
<table class="table table-hover table-condensed table-responsive">
  <thead>
  <th>Circuit</th>
  <th>Locality</th>
  <th>Country</th>
  <th>URL</th>
  </thead>
  <tbody>
  {{#each courses}}
    <tr>
      <td><a href="/courses/{{circuitId}}">{{circuitName}}</a></td>
      <td>{{Location.locality}}</td>
      <td>{{Location.country}}</td>
      <td>
        <a href="{{url}}" rel="external" target="_blank">
          <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span>
        </a>
      </td>
    </tr>
  {{/each}}
  </tbody>
</table>
